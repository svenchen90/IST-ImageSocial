<form action="/signup" method="post">
	username: <input name="username"></input><br>
	password: <input name="password"></input><br>
	first_name: <input name="first_name"></input><br>
	last_name: <input name="last_name"></input><br>
	birthday: <input type="date" name="birthday"></input><br>
	occupations: 
		<select multiple name="occupations">
			<option value="volvo">Volvo</option>
			<option value="saab">Saab</option>
			<option value="opel">Opel</option>
			<option value="audi">Audi</option>
		</select><br>
	citizenships: 
		<select multiple name="citizenships">
			<option value="volvo">Volvo</option>
			<option value="saab">Saab</option>
			<option value="opel">Opel</option>
			<option value="audi">Audi</option>
		</select><br>
	email: <input type="text" name="email"></input><br>
	phone: <input type="text" name="phone"></input><br>
	introduction: <textarea name="introduction"></textarea>,
	<!-- photo: {type: String, default: 'userphoto_default.jpg', required: true},
	following: [{type: ObjectId, ref: 'User',unique: true}],
	status: {type: Number, required: true}, -->
	<button type="submit">submit</button>
</form>